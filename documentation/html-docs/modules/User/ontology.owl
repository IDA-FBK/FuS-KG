    <!-- https://w3id.org/fuskg/user#hasTimestamp -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/fuskg/user#hasTimestamp">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/fuskg/user#Meal"/>
