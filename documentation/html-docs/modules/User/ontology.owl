    <!-- https://w3id.org/fuskg/user#activityDuration -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/fuskg/user#activityDuration">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/fuskg/user#EffortCaloriesEquivalent"/>
