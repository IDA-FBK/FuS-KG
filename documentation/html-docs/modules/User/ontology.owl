                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:comment xml:lang="en">This property specifies the duration of the activity.</rdfs:comment>
        <rdfs:label xml:lang="en">activity duration</rdfs:label>
        <rdfs:label xml:lang="it">durata attivita&apos;</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/fuskg/user#activityEndTimestamp -->


    <owl:DatatypeProperty rdf:about="https://w3id.org/fuskg/user#activityEndTimestamp">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#PerformedActivity"/>
        <rdfs:comment xml:lang="en">This property specifies the timestamp of the end of the activity.</rdfs:comment>
        <rdfs:label xml:lang="en">activity end timestamp</rdfs:label>
        <rdfs:label xml:lang="it">timestamp di fine attivita&apos;</rdfs:label>
