    <!-- https://w3id.org/fuskg/user#belongsProfile -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#belongsProfile">
        <owl:inverseOf rdf:resource="https://w3id.org/fuskg/user#containsUser"/>
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#User"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#Profile"/>
        <rdfs:comment xml:lang="en">This property specifies to which Profile a User belongs.</rdfs:comment>
        <rdfs:label xml:lang="it">appartiena al profilo</rdfs:label>
        <rdfs:label xml:lang="en">belongs to profile</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#consumed -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#consumed">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#User"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#Meal"/>
        <rdfs:comment xml:lang="en">This property specifies what the user has consumed during a Meal. The Meal is defined as a list of ConsumedFood instances indicating the food and the amount eaten.</rdfs:comment>
        <rdfs:label xml:lang="en">consumed</rdfs:label>
        <rdfs:label xml:lang="it">ha consumato</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#containsUser -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#containsUser">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#Profile"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#User"/>
        <rdfs:comment xml:lang="en">This property specifies the user(s) contained in a profile.</rdfs:comment>
        <rdfs:label xml:lang="en">contains user</rdfs:label>
        <rdfs:label xml:lang="it">contiene l&apos;utente</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#hasActivity -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#hasActivity">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/fuskg/user#EffortCaloriesEquivalent"/>
                    <rdf:Description rdf:about="https://w3id.org/fuskg/user#PerformedActivity"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/core#Activity"/>
        <rdfs:comment xml:lang="en">This property specifies which activity the domain instance refers to.</rdfs:comment>
        <rdfs:label xml:lang="it">ha attivita&apos;</rdfs:label>
        <rdfs:label xml:lang="en">has activity</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#hasConsumedFood -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#hasConsumedFood">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#Meal"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#ConsumedFood"/>
        <rdfs:comment xml:lang="en">This property specifies what (ConsumedFood) the user has consumed during a Meal. The ConsumedFood instance indicates the food and the amount eaten.</rdfs:comment>
        <rdfs:label xml:lang="it">ha Consumato Cibo</rdfs:label>
        <rdfs:label xml:lang="en">has Consumed Food</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#hasFood -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#hasFood">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#ConsumedFood"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/core#Food"/>
        <rdfs:comment xml:lang="en">This property specifies which food has been consumed.</rdfs:comment>
        <rdfs:label xml:lang="it">ha Cibo</rdfs:label>
        <rdfs:label xml:lang="en">has Food</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#hasUser -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#hasUser">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#Meal"/>
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#PerformedActivity"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#User"/>
        <rdfs:comment xml:lang="en">This property specifies which user the domain instance refers to. In detail which user performed the activity if the domain instance is a PerformedActivity, and which user ate a meal if the domain instance belongs to Meal.</rdfs:comment>
        <rdfs:label xml:lang="it">ha Utente</rdfs:label>
        <rdfs:label xml:lang="en">has User</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#hasViolationMeal -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#hasViolationMeal">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#Violation"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#Meal"/>
        <rdfs:comment xml:lang="en">This property specifies which Meal triggered the violation.</rdfs:comment>
        <rdfs:label xml:lang="it">ha violazione sul pasto</rdfs:label>
        <rdfs:label xml:lang="en">has violation on Meal</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#hasViolationRule -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#hasViolationRule">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#Violation"/>
        <rdfs:comment xml:lang="en">This property specifies which rule is violated.</rdfs:comment>
        <rdfs:label xml:lang="it">ha violazione sulla regola</rdfs:label>
        <rdfs:label xml:lang="en">has violation on rule</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#hasViolationUser -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#hasViolationUser">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#Violation"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#User"/>
        <rdfs:comment xml:lang="en">This property specifies which User the violation refers to.</rdfs:comment>
        <rdfs:label xml:lang="it">ha violazione per l&apos;utente</rdfs:label>
        <rdfs:label xml:lang="en">has violation User</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/fuskg/user#performed -->


    <owl:ObjectProperty rdf:about="https://w3id.org/fuskg/user#performed">
        <rdfs:domain rdf:resource="https://w3id.org/fuskg/user#User"/>
        <rdfs:range rdf:resource="https://w3id.org/fuskg/user#PerformedActivity"/>
        <rdfs:comment xml:lang="en">This property specifies an activity performed by a user. The PerformedActivity instance indicates the activity the User did and its duration.</rdfs:comment>
        <rdfs:label xml:lang="it">ha eseguito</rdfs:label>
        <rdfs:label xml:lang="en">performed</rdfs:label>
    </owl:ObjectProperty>
    


