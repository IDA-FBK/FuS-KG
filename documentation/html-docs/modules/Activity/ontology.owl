    <owl:DatatypeProperty rdf:about="https://w3id.org/fuskg/multimodal#height">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/fuskg/multimodal#Image"/>
