###  https://w3id.org/fuskg/temporal#afterStep
fstemp:afterStep rdf:type owl:ObjectProperty ;
                 rdfs:domain fstemp:TemporalStep ;
                 rdfs:range fstemp:TemporalStep ;
                 rdfs:comment "Property used to connect a TemporalStep with the next one."@en ,
                              "after step"@en ,
                              "step successivo"@it .


###  https://w3id.org/fuskg/temporal#beforeStep
fstemp:beforeStep rdf:type owl:ObjectProperty ;
                  rdfs:domain fstemp:TemporalStep ;
                  rdfs:range fstemp:TemporalStep ;
                  rdfs:comment "Property used to connect a TemporalStep with the previous one."@en ;
                  rdfs:label "before step"@en ,
                             "step precedente"@it .


###  https://w3id.org/fuskg/temporal#hasFirstRecipeStep
fstemp:hasFirstRecipeStep rdf:type owl:ObjectProperty ;
                          rdfs:domain fsreci:Recipe ;
                          rdfs:range fstemp:RecipeStep ;
                          rdfs:comment "Property used to connect a Recipe with its first RecipeStep."@en ;
                          rdfs:label "ha step iniziale della ricetta"@it ,
                                     "has first recipe step"@en .


###  https://w3id.org/fuskg/temporal#hasStepDescription
fstemp:hasStepDescription rdf:type owl:ObjectProperty ;
                          rdfs:domain fstemp:TemporalStep ;
                          rdfs:range fstemp:StepDescription ;
                          rdfs:comment "Property used to connect a Temporal Step with its corresponding StepDescription."@en ;
                          rdfs:label "ha descrizione dello Step"@it ,
                                     "has Step Description"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/fuskg/temporal#duration
fstemp:duration rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf time:numericDuration ;
                rdfs:label "durata"@it ,
                           "duration"@en .


###  https://w3id.org/fuskg/temporal#orderedStepBegin
fstemp:orderedStepBegin rdf:type owl:DatatypeProperty ;
                        rdfs:domain fstemp:TemporalStep ;
                        rdfs:range xsd:float ;
                        rdfs:comment "Begin (frame or seconds) of a TemporalStep."@en ;
                        rdfs:label "begin temporal step"@en ,
                                   "inizio step temporale"@it .


###  https://w3id.org/fuskg/temporal#orderedStepEnd
fstemp:orderedStepEnd rdf:type owl:DatatypeProperty ;
                      rdfs:domain fstemp:TemporalStep ;
                      rdfs:range xsd:float ;
                      rdfs:comment "End (frame or seconds) of a TemporalStep."@en ;
                      rdfs:label "fine step temporale"@it ,
                                 "temporal step end"@en .


###  https://w3id.org/fuskg/temporal#stepDescription
fstemp:stepDescription rdf:type owl:DatatypeProperty ;
                       rdfs:domain fstemp:StepDescription ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Text description of an instance of StepDescription." ;
                       rdfs:label "description step"@en ,
                                  "descrizione step"@it .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/fuskg/recipe#Recipe
fsreci:Recipe rdf:type owl:Class .


###  https://w3id.org/fuskg/temporal#Day
fstemp:Day rdf:type owl:Class ;
           rdfs:subClassOf fstemp:Timespan ;
           rdfs:comment "Concept defigning a time period of one day."@en ;
           rdfs:label "Day"@en ,
                      "Giornata"@it .


###  https://w3id.org/fuskg/temporal#Meal
fstemp:Meal rdf:type owl:Class ;
            rdfs:subClassOf time:Instant ;
            rdfs:comment "This concept enables the definition of a Meal as the time a person eats exploited for monitoring users."@en ;
            rdfs:label "Meal"@en ,
                       "Pasto"@it .


###  https://w3id.org/fuskg/temporal#RecipeStep
fstemp:RecipeStep rdf:type owl:Class ;
                  rdfs:subClassOf fstemp:TemporalStep ;
                  rdfs:comment "This concept is used to represent a step of a recipe."@en ;
                  rdfs:label "Recipe step"@en ,
                             "Step ricetta"@it .


###  https://w3id.org/fuskg/temporal#StepDescription
fstemp:StepDescription rdf:type owl:Class ;
                       rdfs:comment "This concept is used to represent a generic description step."@en ;
                       rdfs:label "Descrizione step"@it ,
                                  "Step description"@en .


###  https://w3id.org/fuskg/temporal#TemporalStep
fstemp:TemporalStep rdf:type owl:Class ;
                    rdfs:subClassOf time:ProperInterval ;
                    rdfs:comment "This concept defines generic temporal steps."@en ;
                    rdfs:label "Step temporale"@it ,
                               "Temporal step"@en .


###  https://w3id.org/fuskg/temporal#Timespan
fstemp:Timespan rdf:type owl:Class ;
                rdfs:subClassOf time:ProperInterval ;
                rdfs:comment "This concept enables the definition of custom time period exploited for monitoring users."@en ;
                rdfs:label "Intervallo Temporale"@it ,
                           "Timespan"@en .


###  https://w3id.org/fuskg/temporal#Week
fstemp:Week rdf:type owl:Class ;
            rdfs:subClassOf fstemp:Timespan ;
            rdfs:comment "Concept defigning a time period of one week."@en ;
            rdfs:label "Settimana"@it ,
                       "Week"@en .


