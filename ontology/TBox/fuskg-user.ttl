###  https://w3id.org/fuskg/user#belongsProfile
fsuser:belongsProfile rdf:type owl:ObjectProperty ;
                      owl:inverseOf fsuser:containsUser ;
                      rdfs:domain fsuser:User ;
                      rdfs:range fsuser:Profile ;
                      rdfs:comment "This property specifies to which Profile a User belongs."@en ;
                      rdfs:label "appartiena al profilo"@it ,
                                 "belongs to profile"@en .


###  https://w3id.org/fuskg/user#consumed
fsuser:consumed rdf:type owl:ObjectProperty ;
                rdfs:domain fsuser:User ;
                rdfs:range fsuser:Meal ;
                rdfs:comment "This property specifies what the user has consumed during a Meal. The Meal is defined as a list of ConsumedFood instances indicating the food and the amount eaten."@en ;
                rdfs:label "consumed"@en ,
                           "ha consumato"@it .


###  https://w3id.org/fuskg/user#containsUser
fsuser:containsUser rdf:type owl:ObjectProperty ;
                    rdfs:domain fsuser:Profile ;
                    rdfs:range fsuser:User ;
                    rdfs:comment "This property specifies the user(s) contained in a profile."@en ;
                    rdfs:label "contains user"@en ,
                               "contiene l'utente"@it .


###  https://w3id.org/fuskg/user#hasActivity
fsuser:hasActivity rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( fsuser:EffortCaloriesEquivalent
                                               fsuser:PerformedActivity
                                             )
                               ] ;
                   rdfs:range fscore:Activity ;
                   rdfs:comment "This property specifies which activity the domain instance refers to."@en ;
                   rdfs:label "ha attivita'"@it ,
                              "has activity"@en .


###  https://w3id.org/fuskg/user#hasConsumedFood
fsuser:hasConsumedFood rdf:type owl:ObjectProperty ;
                       rdfs:domain fsuser:Meal ;
                       rdfs:range fsuser:ConsumedFood ;
                       rdfs:comment "This property specifies what (ConsumedFood) the user has consumed during a Meal. The ConsumedFood instance indicates the food and the amount eaten."@en ;
                       rdfs:label "ha Consumato Cibo"@it ,
                                  "has Consumed Food"@en .


###  https://w3id.org/fuskg/user#hasFood
fsuser:hasFood rdf:type owl:ObjectProperty ;
               rdfs:domain fsuser:ConsumedFood ;
               rdfs:range fscore:Food ;
               rdfs:comment "This property specifies which food has been consumed."@en ;
               rdfs:label "ha Cibo"@it ,
                          "has Food"@en .


###  https://w3id.org/fuskg/user#hasUser
fsuser:hasUser rdf:type owl:ObjectProperty ;
               rdfs:domain fsuser:Meal ,
                           fsuser:PerformedActivity ;
               rdfs:range fsuser:User ;
               rdfs:comment "This property specifies which user the domain instance refers to. In detail which user performed the activity if the domain instance is a PerformedActivity, and which user ate a meal if the domain instance belongs to Meal."@en ;
               rdfs:label "ha Utente"@it ,
                          "has User"@en .


###  https://w3id.org/fuskg/user#hasViolationMeal
fsuser:hasViolationMeal rdf:type owl:ObjectProperty ;
                        rdfs:domain fsuser:Violation ;
                        rdfs:range fsuser:Meal ;
                        rdfs:comment "This property specifies which Meal triggered the violation."@en ;
                        rdfs:label "ha violazione sul pasto"@it ,
                                   "has violation on Meal"@en .


###  https://w3id.org/fuskg/user#hasViolationRule
fsuser:hasViolationRule rdf:type owl:ObjectProperty ;
                        rdfs:domain fsuser:Violation ;
                        rdfs:comment "This property specifies which rule is violated."@en ;
                        rdfs:label "ha violazione sulla regola"@it ,
                                   "has violation on rule"@en .


###  https://w3id.org/fuskg/user#hasViolationUser
fsuser:hasViolationUser rdf:type owl:ObjectProperty ;
                        rdfs:domain fsuser:Violation ;
                        rdfs:range fsuser:User ;
                        rdfs:comment "This property specifies which User the violation refers to."@en ;
                        rdfs:label "ha violazione per l'utente"@it ,
                                   "has violation User"@en .


###  https://w3id.org/fuskg/user#performed
fsuser:performed rdf:type owl:ObjectProperty ;
                 rdfs:domain fsuser:User ;
                 rdfs:range fsuser:PerformedActivity ;
                 rdfs:comment "This property specifies an activity performed by a user. The PerformedActivity instance indicates the activity the User did and its duration."@en ;
                 rdfs:label "ha eseguito"@it ,
                            "performed"@en .


