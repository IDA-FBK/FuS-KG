###  https://w3id.org/fuskg/multimodal#hasModalDescriptor
fsmumo:hasModalDescriptor rdf:type owl:ObjectProperty ;
                          rdfs:domain fsmumo:ModalEntity ;
                          rdfs:range fsmumo:ModalDescriptor ;
                          rdfs:comment "Property used to connect a ModalEntity with its corresponding ModalDescriptor(s)."@en ;
                          rdfs:label "ha descrittore modale"@it ,
                                     "has modal descriptor" .


###  https://w3id.org/fuskg/multimodal#hasModality
fsmumo:hasModality rdf:type owl:ObjectProperty ;
                   rdfs:domain fsmumo:ModalDescriptor ;
                   rdfs:range fsmumo:Modality ;
                   rdfs:comment "Property used to connect a ModalDescriptor with its correspondig Modality."@en ;
                   rdfs:label "ha modalita'"@it ,
                              "has modality"@en .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/fuskg/multimodal#fps
fsmumo:fps rdf:type owl:DatatypeProperty ;
           rdfs:domain fsmumo:Video ;
           rdfs:range xsd:float ;
           rdfs:comment "Frame per second of a Video."@en ;
           rdfs:label "Frame al secondo"@it ,
                      "Frame per second"@en .


###  https://w3id.org/fuskg/multimodal#hasPathInArtifact
fsmumo:hasPathInArtifact rdf:type owl:DatatypeProperty ;
                         rdfs:domain fsmumo:ModalDescriptor ;
                         rdfs:range xsd:string ;
                         rdfs:comment "Path inside an artifact (e.g., an archive)."@en ;
                         rdfs:label "Path inside artifact"@en ,
                                    "Percorso dentro artefatto"@it .


###  https://w3id.org/fuskg/multimodal#height
fsmumo:height rdf:type owl:DatatypeProperty ;
              rdfs:domain [ rdf:type owl:Class ;
                            owl:unionOf ( fsmumo:Image
                                          fsmumo:Video
                                        )
                          ] ;
              rdfs:range xsd:int ;
              rdfs:comment "Height of an image or video (related to resolution)."@en ;
              rdfs:label "altezza"@it ,
                         "height"@en .


###  https://w3id.org/fuskg/multimodal#resourceURL
fsmumo:resourceURL rdf:type owl:DatatypeProperty ;
                   rdfs:domain fsmumo:ModalDescriptor ;
                   rdfs:range xsd:anyURI ;
                   rdfs:comment "URL pointing to a resource."@en ;
                   rdfs:label "URL resource"@en ,
                              "URL risorsa"@it .


###  https://w3id.org/fuskg/multimodal#width
fsmumo:width rdf:type owl:DatatypeProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( fsmumo:Image
                                         fsmumo:Video
                                       )
                         ] ;
             rdfs:range xsd:int ;
             rdfs:comment "Width of an image or video (related to resolution)."@en ;
             rdfs:label "larghezza"@it ,
                        "width"@en .


