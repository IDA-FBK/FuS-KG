#    Classes
#################################################################

###  http://www.w3.org/2006/time#ProperInterval
<http://www.w3.org/2006/time#ProperInterval> rdf:type owl:Class .


###  https://w3id.org/fuskg/multimodal#Audio
fsmumo:Audio rdf:type owl:Class ;
             rdfs:subClassOf fsmumo:Modality ;
             rdfs:comment "Concept denoting Audio modality."@en ;
             rdfs:label "Audio"@en ,
                        "Audio"@it .


###  https://w3id.org/fuskg/multimodal#DataStream
fsmumo:DataStream rdf:type owl:Class ;
                  rdfs:subClassOf fsmumo:Modality ;
                  rdfs:comment "Concept denoting DataStream modality."@en ;
                  rdfs:label "Data stream"@en ,
                             "Stream dati"@it .


###  https://w3id.org/fuskg/multimodal#Image
fsmumo:Image rdf:type owl:Class ;
             rdfs:subClassOf fsmumo:Modality ;
             rdfs:comment "Concept denoting Image modality."@en ;
             rdfs:label "Image"@en ,
                        "Immagine"@it .


###  https://w3id.org/fuskg/multimodal#ModalDescriptor
fsmumo:ModalDescriptor rdf:type owl:Class ;
                       rdfs:subClassOf <http://www.w3.org/ns/dcat#Resource> ;
                       rdfs:comment "Concept used to connect a modal entity with its modality."@en ;
                       rdfs:label "Descrittore modale"@it ,
                                  "Modal descriptor"@en .


###  https://w3id.org/fuskg/multimodal#ModalEntity
fsmumo:ModalEntity rdf:type owl:Class ;
                   rdfs:comment "Root class used to specify modal entities that can be associated with one or more modal resources."@en ;
                   rdfs:label "Entita' Modale"@it ,
                              "Modal Entity"@en .


###  https://w3id.org/fuskg/multimodal#Modality
fsmumo:Modality rdf:type owl:Class ;
                rdfs:comment "Parent class used to specify a data modality of a resource"@en ;
                rdfs:label "Modalita'"@it ,
                           "Modality"@en .


###  https://w3id.org/fuskg/multimodal#Text
fsmumo:Text rdf:type owl:Class ;
            rdfs:subClassOf fsmumo:Modality ;
            rdfs:comment "Concept denoting Text modality."@en ;
            rdfs:label "Testo"@it ,
                       "Text"@en .


###  https://w3id.org/fuskg/multimodal#Video
fsmumo:Video rdf:type owl:Class ;
             rdfs:subClassOf fsmumo:Modality ;
             rdfs:comment "Concept denoting Video modality."@en ;
             rdfs:label "Video"@en ,
                        "Video"@it .


###  https://w3id.org/fuskg/recipe#Recipe
fsreci:Recipe rdfs:subClassOf fsmumo:ModalEntity .


#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( fsmumo:Audio
                fsmumo:DataStream
                fsmumo:Image
                fsmumo:Text
                fsmumo:Video
              )
] .


